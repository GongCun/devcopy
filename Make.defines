# Common make definitions.

CC = gcc
LDDIR = $(ROOT)/lib
LIBMYADT = $(LDDIR)/libmyadt.a
LIBMYERR = $(LDDIR)/libmyerr.a
LDFLAGS = -L$(LDDIR) 
LDLIBS = -lmyadt -lmyerr $(EXTRALIBS)
CFLAGS = -g -Wall -Wextra $(EXTRA) -g
COMPILE.c = $(CC) $(CFLAGS) $(CPPFLAGS) -c
LINK.c = $(CC) $(CFLAGS) $(LDFLAGS)
AR = ar
TEMPFILES = core core.* *.o temp.* *.out *~

